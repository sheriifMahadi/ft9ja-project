 "builds": [
      {
        "src": "ft9ja_project/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "static"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "ft9ja_project/wsgi.py"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      }
    ],
    "outputDirectory": "static"
